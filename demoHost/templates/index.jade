doctype html
html(lang="en")
  head
    title= pageTitle
    script(type='text/javascript' src=).

  body(class="main-content")
    h1 Component Demo Index

    div
      each mainSectionKey in Object.keys(structure)
        div= mainSectionKey
        each subSectionKey in Object.keys(structure[mainSectionKey])
          a(href='/component/' + mainSectionKey + '/' + subSectionKey + '/' + structure[mainSectionKey][subSectionKey] + '.demo.html') #{subSectionKey}
          br